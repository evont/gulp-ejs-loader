<script type="text/x-template" id="tpl_tab">
  <% if(model.tab.length) { %>
  <div class="j-ind-menubar dIndex-entry">
    <% for (var i = 0, barcount = model.tab.length; i < barcount; i++) { var tabItem = model.tab[i];%>
    <a class="dIndex-entry-item" href="<%= tabItem.link %>" data-bartype="<%= tabItem.text %>">
        <span class="">
            <img src="<%= model.tab[i].img %>"><%= tabItem.text %>
        </span>
    </a>
    <% }%>
  </div>
  <% }%>
</script>

<script type="text/x-template" id="tpl_takingon">
  <% if (model.zucaitakingon && model.zucaitakingon.length > 0) {%>
    <% var issueList = model.zucaiissue || []; var issue = issueList.indexOf(model.issue) === -1 ? issueList[0] : model.issue;%>
    <h2 class="dIndex-title">
      当期对阵
      <div class="dIndex-title-action">
        <div class="dMod-select">
          <select class="j-issue-select" name="issue">
            <% for (var i = 0, len = issueList.length; i < len; i += 1) { 
              var selected = false;
              if (issue === issueList[i]) {
                selected = true;
              }
            %>
            <option value="<%= issueList[i] %>" <% if (selected) { %>selected="<% } %>"><%= issueList[i] %>期</option>
            <% } %>
          </select>
          <span class="dMod-select-text"><span class="j-issue-option"><%= issue %></span>期</span>
        </div>
      </div>
    </h2>
    <div class="j-takingon-list dIndex-takingon">
      <div class="dIndex-takingon-list" data-status="0">
        <% for (var i = 0, len =  model.zucaitakingon.length; i < len; i++) { var item = model.zucaitakingon[i]; %>
          <div class="dIndex-takingon-item">
            <div class="dSub-team dSub-team_fTop">
              <div class="dSub-team-name"><span><%= item.hometeam.substr(0, 4) %></span></div>
              <div class="dSub-team-pic">
                <img src="<%= item.homepic %>">
              </div>
            </div>
            <span class="dSub-order"><%= i + 1 %></span>
            <div class="dSub-team dSub-team_fBottom">
              <div class="dSub-team-pic">
                <img src="<%= item.guestpic %>">
              </div>
              <div class="dSub-team-name"><span><%= item.guestteam.substr(0, 4) %></span></div>
            </div>
          </div>
        <% }%>
      </div>
    </div>
  <% }%>
</script>

<script type="text/x-template" id="tpl_discountpackage">
<% if (model.discountpackage && model.discountpackage.length > 0) {%>
  <h2 class="dIndex-title">包周套餐 不中退款</h2>
  <div class="dIndex-discountPkg">
      <% for(var ind = 0, len = model.discountpackage.length; ind < len; ind++) { 
        var ele = model.discountpackage[ind];
     %>
          <% if(ele) {%>
          <a class="dIndex-discountPkg-item" data-index="<%= ind %>" href="/DiscountPackage/number_detail/<%= gamename %>/<%= ele.memberid %>.html">
              <div class="dSub-avatar">
                  <img class="lazy" src="https://dr.vipc.cn/images/daren/user-default.png?v=9c59a03da8" data-original="<%= ele.pic %>">
                  <span class="dSub-level dIcon-expertLevel" data-level="<%= ele.level %>"></span>
              </div>
              <div class="dSub-awardlist">
                  <% if(ele.awardlist && ele.awardlist[0]){ %>
                      <p><span class="award-text award-text_pBold"><%= ele.awardlist ? ele.awardlist[0] : '' %></span></p>
                  <% } %>
                  <% if(ele.awardlist && (ele.awardlist[1] || ele.awardlist[2])){ %>
                      <p>
                          <span class="award-text"><%= ele.awardlist[1] ? ele.awardlist[1] : ele.awardlist[2] %></span>
                      </p>
                  <% } %>
              </div>
              <div class="dSub-action">
                  <p class="dSub-action-check">立即查看</p>
                  <p class="dSub-action-price"><%= ele.price %>钻石 / <%= /ssq|ChaoJiDaLeTou/i.test(gamename) ? 3 : 7 %>期</p>
              </div>
          </a>
          <% }%>
      <% }%>
  </div>
  <p class="dIndex-subTitle">包周栏目享受不中退款服务</p>
 <% }%>
</script>
