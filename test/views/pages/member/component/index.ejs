<script type="text/x-template" id="tpl_header">
  <% var isSporttrey = config.isSporttrey(gamename);
  var navList = [];
    if (isSporttrey) {
      navList = [
        {type: 'Sporttrey320', name: '竞足'},
        {type: 'Sporttrey307', name: '竞篮'},
        {type: 'ZuCai14', name: '胜负彩'},
      ];
    } else {
      navList = [
        {type: 'ssq', name: '双色球'},
        {type: 'ChaoJiDaLeTou', name: '大乐透'}, 
        {type: '3d', name: '福彩3D'},
        {type: 'PaiLieSan', 'name': '排列三'}
      ];
    }
    %>
  <div class="dIndex-header">
    <div class="dIndex-header-logo">
      <img src="/img/logo_pWhite.png?v=71767d55f7">
    </div>
    <div class="dIndex-header-middle">
      <div class="dMod-toggler dMod-toggler_pRev">
        <a class="dMod-toggler-item" data-status="<%= isSporttrey ? 1 : 0 %>" href="/index/Sporttrey320.html">竞技彩</a>
        <a class="dMod-toggler-item" data-status="<%= isSporttrey ? 0 : 1 %>" href="/index/ssq.html">数字彩</a>
      </div>
    </div>
  </div>
  <div class="dMod-nav dMod-nav_fTab dMod-nav_pNoBottom">
    <% for (var i = 0, len = navList.length; i < len; i += 1) { var item = navList[i];%>
      <a class="dMod-nav-item" data-status="<%= item.type == gamename ? 1 : 0 %>" href="/index/<%= item.type %>.html"><%= item.name %></a>
    <% } %>
  </div>
</script>
<script type="text/x-template" id="tpl_banner">
  <% if (model.active != null && model.active.length > 0 ) { var length = model.active.length;%>
  
    <ul class="j-slider-box dMod-slider-box dFun-clearboth dMod-slider_pTrans" style="width: <%= length * 100 %>%">
      <% for (var i = 0; i < length; i++) { var activeItem = model.active[i];%> 
        <li style="width: <%= 100 / length %>%">
          <a href="<%= (/advert\.action(\?|\S*&)type=1/.test(activeItem.url)&& !addition.token) ? ('/login.html?url=' + encodeURIComponent(activeItem.url.replace(/&?type=1/,''))) : activeItem.url.replace(/&?type=0/,'') %>">
            <img src="<%= activeItem.img %>" />
          </a>
        </li>
      <% }%>
    </ul>
    <div class="j-slider-dot dMod-slider-dot hide"></div>
  <% }%>
</script>

<script type="text/x-template" id="tpl_hotmaster">
  <% if (model.hotmaster != null && gamename !== 'ZuCai14') {%>
  
    <h2 class="dIndex-title"><%= config.isSporttrey(gamename) ? '特约分析师' : '热门达人' %></h2>
    <div class="dIndex-box">
      <div class="j-slider dMod-slider dIndex-hotmaster" data-type="hotmaster">
        <ul class="dMod-slider-box dIndex-hotmaster-box dFun-clearboth">
          <li>
            <% for (var j = 0; j < model.hotmaster.length; j++){ var hot = model.hotmaster[j]; if(j > 6) { break; }%>
              <span class="j-darenItem dSub-darenItem" data-href="/DaRenDetail/<%= hot.MemberId %>/<%= gamename %>.html?iid=10" data-paymaxid="<%= hot.paymaxid %>"
                data-memberid="<%= hot.MemberId %>" data-isnew="<%= hot.paycount > 0 ? 1 : 0 %>">
                <div class="dSub-avatar">
                  <img class="lazy" src="https://dr.vipc.cn/images/daren/user-default.png?v=9c59a03da8" data-original="<%=hot.Pic %>">
                </div>
                <p class="ellipsis dSub-nickname"><%= hot.NickName %></p>
                <p class="ellipsis dSub-remark"><%= hot.Remark %></p>
              </span>
              <% }%>
              <a class="dSub-darenItem" href="<%= config.isSporttrey(gamename) ? '/SpecialList/' + gamename + '.html' : '/expert/' + gamename + '.html' %>">
                <div class="dSub-avatar">
                  <img src="/img/moremaster.png?v=15b1efd467">
                </div>
                <p class="ellipsis dSub-nickname">更多</p>
                <p class="ellipsis dSub-remark"></p>
              </a>
          </li>
        </ul>
      </div>
    </div>
  </div>
  <% }%>
</script>

<script type="text/x-template" id="tpl_rollrecord">
  <% if (model.kuaixun != null) {%>
    <div class="j-rollrecord dMod-payRecord dMod-payRecord_aIndex">
